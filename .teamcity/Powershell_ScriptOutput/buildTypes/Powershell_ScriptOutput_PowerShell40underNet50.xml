<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="57095a52-efd7-4b64-995f-bb37be8d1aae" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>PowerShell 4.0 under .Net 5.0</name>
  <description />
  <settings>
    <options>
