      <runner id="RUNNER_172" name="Source Code &amp; &quot;-File&quot;" type="jetbrains_powershell">
        <parameters>
          <param name="jetbrains_powershell_bitness" value="x86" />
          <param name="jetbrains_powershell_execution" value="PS1" />
          <param name="jetbrains_powershell_minVersion" value="4.0" />
          <param name="jetbrains_powershell_script_code" value="Write-Output &quot;Write-Output output from Source and File&quot;" />
          <param name="jetbrains_powershell_script_mode" value="CODE" />
          <param name="teamcity.step.mode" value="execute_if_failed" />
        </parameters>
      </runner>
      <runner id="RUNNER_173" name="File &amp; &quot;-Command-&quot;" type="jetbrains_powershell">
